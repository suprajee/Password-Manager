{
    "manifest_version": 3,
    "name": "Password Manager",
    "version": "1.0",
    "description": "A simple password manager extension.",
    "permissions": [
        "storage",
        "activeTab",
        "scripting",
        "tabs"
    ],
    "host_permissions": ["https://*/*", "http://*/*"],
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icon16.png",
        "48": "icon48.png",
        "128": "icon128.png"
      }
    },
    "background": {
      "service_worker": "background.js"
    },
    "content_scripts": [
      {
        "matches": ["https://*/*", "http://*/*"],
        "js": ["autofill.js", "content.js"],
        "run_at": "document_end"
      }
    ]
}
  